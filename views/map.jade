                    map: map,
                    animation: google.maps.Animation.DROP,
                    title: element.name,
                    icon: yellowImage, //'/images/marker-yellow-det.png',
                    zIndex: 1
                });
                marker.element = element;

                marker.addListener('click', function () {
                    infowindow.open(map, marker);
                });
                markers.push(marker);
            }
        });
